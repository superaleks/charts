{
  "phases": {
    "package": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/bitnami/dokuwiki"
        }
      },
      "actions": [
        {
          "action_id": "helm-package"
        },
        {
          "action_id": "helm-lint"
        }
      ]
    },
    "verify": {
      "context": {
        "resources": {
          "url": "{SHA_ARCHIVE}",
          "path": "/bitnami/dokuwiki"
        },
        "runtime_parameters": "ZG9rdXdpa2lVc2VybmFtZTogc3VwZXJ1c2VyCmRva3V3aWtpUGFzc3dvcmQ6IGJpdG5hbWkxCnNlcnZpY2U6CiAgdHlwZTogTG9hZEJhbGFuY2VyCiAgcG9ydHM6CiAgICBodHRwOiA4MA==",
        "target_platform": {
          "target_platform_id": "{VIB_ENV_TARGET_PLATFORM}",
          "size": {
            "name": "S4"
          }
        }
      },
      "actions": [
        {
          "action_id": "trivy",
          "params": {
            "threshold": "CRITICAL",
            "vuln_type": ["OS"]
          }
        },
        {
          "action_id": "health-check",
          "params": {
            "endpoint": "lb-dokuwiki-http",
