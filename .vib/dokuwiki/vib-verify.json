            }
          }
        },
        {
          "action_id": "goss",
          "params": {
            "resources": {
              "path": "/.vib/dokuwiki/goss"
            },
            "remote": {
              "workload": "deploy-dokuwiki"
            }
          }
        }
      ]
    }
  }
}
